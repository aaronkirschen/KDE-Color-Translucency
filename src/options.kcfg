            <default>black</default>
        </entry>

        <entry name="TargetAlpha_10" type="Int">
            <label>Transparent Color 10 Alpha</label>
            <default>0</default>
        </entry>

        <entry name="InclusionList" type="StringList">
            <label>Included Windows</label>
            <default></default>

        </entry>

        <entry name="ExclusionList" type="StringList">
            <label>Excluded Windows</label>
            <default></default>
        </entry>

    </group>

</kcfg>