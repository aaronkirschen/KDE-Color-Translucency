            <default>black</default>
        </entry>

        <entry name="TargetAlpha_5" type="Int">
            <label>Transparent Color 5 Alpha</label>
            <default>0</default>
        </entry>

        <entry name="TargetColor_6" type="Color">
            <label>Transparent Color 6</label>
            <default>black</default>
        </entry>

        <entry name="TargetAlpha_6" type="Int">
            <label>Transparent Color 6 Alpha</label>
            <default>0</default>
        </entry>

        <entry name="TargetColor_7" type="Color">
