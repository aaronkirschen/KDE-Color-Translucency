            <default>false</default>
        </entry>

        <entry name="EnableColor_10" type="Bool">
            <label>Checkbox enable color/alpha 10</label>
            <default>false</default>
        </entry>

        <entry name="TargetColor_1" type="Color">
            <label>Transparent Color 1</label>
            <default>black</default>
        </entry>

        <entry name="TargetAlpha_1" type="Int">
            <label>Transparent Color 1 Alpha</label>
            <default>0</default>
        </entry>

        <entry name="TargetColor_2" type="Color">
            <label>Transparent Color 2</label>
            <default>black</default>
        </entry>

        <entry name="TargetAlpha_2" type="Int">
            <label>Transparent Color 2 Alpha</label>
            <default>0</default>
        </entry>

        <entry name="TargetColor_3" type="Color">
